<div class="container">
  <h1>会員一覧</h1>
  <div class="container">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr>
      </thead>
      <% @customers.each do |customer| %>
      <tbody>
        <tr>
          <td><%= customer.id %> </td>
          <td><%= customer.last_name+ "" +customer.first_name %></td>
          <td><%= customer.email %></td>
          <td># ステータス</td>
        </tr>
      </tbody>
      <% end %>
    </table>
  </div>
</div>
